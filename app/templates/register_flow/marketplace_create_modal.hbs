		labelText="Name"
		field="name"
	}}

	{{view "form-fields/text-form-field" model=view.model inputClassNames="full"
		labelText="Domain url"
		field="domain_url"
		explanationText="http://www.example.com"
	}}

	{{view "form-fields/text-form-field" model=view.model inputClassNames="full"
		labelText="Support email address"
		field="support_email_address"
	}}

	{{view "form-fields/text-form-field" model=view.model inputClassNames="full"
		labelText="Support phone number"
		field="support_phone_number"
	}}
{{/view}}

