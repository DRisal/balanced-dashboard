{{#view Balanced.FormSectionView metaFields=view.metaDictionary.fields}}
	{{#each metaField in view.metaFields}}
		{{view Balanced.MetaFormFieldView
			model=metaField
			labelText="Key"
			field="key"
			inputClassNames="full"
		}}

		{{view Balanced.MetaFormFieldView
			model=metaField
			labelText="Value"
			field="value"
			inputClassNames="full"
		}}
	{{/each}}
	<a {{action 'addNewField' target=view.parentView}}>Add another field</a>
{{/view}}
